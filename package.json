    "@vue/vue3-jest": "^27.0.0-alpha.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "cypress": "^11.2.0",
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.3.0",
